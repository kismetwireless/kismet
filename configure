            BACKTRACE_WARNING="partial - Missing libdw or libbfd will not print full stacks on crash"
        else
            BACKTRACE_OK=0
            BACKTRACE_WARNING="no - Backtraces will not be available on crashes"

printf "%s\n" "#define DISABLE_BACKWARD 1" >>confdefs.h

        fi
    fi
